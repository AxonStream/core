{
  "name": "@axonstream/core",
  "version": "2.0.0",
  "description": "ðŸš€ Game-Changing Real-Time Collaboration Platform - The most advanced real-time messaging SDK with Magic collaboration, self-healing, and universal framework support",
  "keywords": [
    "real-time",
    "collaboration",
    "websocket",
    "operational-transform",
    "time-travel",
    "presence",
    "multi-tenant",
    "rbac",
    "self-healing",
    "auto-reconnect",
    "event-streaming",
    "redis-streams",
    "framework-agnostic",
    "universal-sdk"
  ],
  "author": "AxonStreamAI <info@axonstream.ai>",
  "license": "MIT",
  "homepage": "https://axonstream.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/axonstream/axonpuls-platform.git"
  },
  "bugs": {
    "url": "https://github.com/axonstream/axonpuls-platform/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cdn": "./dist/cdn.min.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts src/vue.ts src/angular.ts src/svelte.ts src/magic.ts src/presence.ts src/webhooks.ts --format esm,cjs --dts --target es2020 --minify",
    "build:cdn": "tsup src/cdn.ts --format iife --minify --dts --target es2020",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm run build:cdn"
  },
  "dependencies": {
    "socket.io-client": "^4.7.4",
    "redis": "^4.6.12",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "eventemitter2": "^6.4.9"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}