{
  "name": "@axonstream/sdk",
  "version": "1.0.0",
  "description": "Core SDK for the AXONPULS Real-Time Messaging Platform by AxonStreamAI",
  "author": "AxonStreamAI <info@axonstream.ai>",
  "homepage": "https://axonstream.ai",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "axonpuls",
    "react",
    "hooks",
    "real-time",
    "websocket",
    "events",
    "axonstreamai",
    "hitl",
    "human-in-the-loop"
  ],
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "ts-jest": "^29.1.2",
    "tsup": "^8.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@axonstream/core": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}