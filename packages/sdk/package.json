{
  "name": "@axonstream/core",
  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapters/vue": {
      "types": "./dist/adapters/vue.d.ts",
      "import": "./dist/adapters/vue.js",
      "require": "./dist/adapters/vue.cjs"
    },
    "./adapters/angular": {
      "types": "./dist/adapters/angular.d.ts",
      "import": "./dist/adapters/angular.js",
      "require": "./dist/adapters/angular.cjs"
    },
    "./adapters/react": {
      "types": "./dist/adapters/react.d.ts",
      "import": "./dist/adapters/react.js",
      "require": "./dist/adapters/react.cjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./embed": {
      "types": "./dist/embed/index.d.ts",
      "import": "./dist/embed/index.js",
      "require": "./dist/embed/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "vue": "^3",
    "@angular/core": "^17 || ^18",
    "@angular/common": "^17 || ^18",
    "rxjs": "^7 || ^8"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test:local": "npm run build && node test-sdk-local.mjs"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "jwt-decode": "^4.0.0",
    "socket.io-client": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.9.2"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
