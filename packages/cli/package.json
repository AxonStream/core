{
    "name": "@axonstream/cli",
    "version": "1.0.0",
    "description": "Command-line interface for AXONPULS Real-Time Messaging Platform by AxonStreamAI",
    "author": "AxonStreamAI <info@axonstream.ai>",
    "homepage": "https://axonstream.ai",
    "main": "dist/index.js",
    "bin": {
        "axonpuls": "dist/cli.js"
    },
    "scripts": {
        "build": "tsup src/cli.ts --format cjs --dts --target node16",
        "dev": "tsup src/cli.ts --format cjs --watch --target node16",
        "start": "node dist/cli.js",
        "lint": "eslint 'src/**/*.{ts,tsx}'"
    },
    "keywords": [
        "axonstream",
        "cli",
        "real-time",
        "websocket",
        "multi-tenant"
    ],
    "dependencies": {
        "@axonstream/core": "*",
        "commander": "^11.1.0",
        "chalk": "^4.1.2",
        "inquirer": "^9.2.12",
        "ora": "^5.4.1",
        "table": "^6.8.1",
        "axios": "^1.6.0",
        "ws": "^8.14.2",
        "dotenv": "^16.3.1",
        "jwt-decode": "^4.0.0"
    },
    "devDependencies": {
        "@types/inquirer": "^9.0.7",
        "@types/ws": "^8.5.8",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ]
